{
  "name": "istanbul-lib-processinfo",
  "version": "1.0.0",
  "description": "A utility for managing the `processinfo` folder that NYC uses.",
  "main": "index.js",
  "scripts": {
    "test": "tap -j1 --cov test/*.js",
    "snap": "cross-env TAP_SNAPSHOT=1 tap -j1 --cov test/*.js",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git push origin --all; git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istanbuljs/istanbul-lib-processinfo.git"
  },
  "license": "ISC",
  "dependencies": {
    "archy": "^1.0.0",
    "cross-spawn": "^6.0.5",
    "istanbul-lib-coverage": "^2.0.3",
    "make-dir": "^2.1.0",
    "rimraf": "^2.6.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "nyc": "^14.0.0",
    "tap": "^12.6.1",
    "which": "^1.3.1"
  },
  "tap": {
    "check-coverage": true
  },
  "files": [
    "index.js"
  ],
  "nyc": {
    "include": "index.js"
  }
}
